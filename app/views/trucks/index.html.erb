<h1>Index page for trucks</h1>


<h1>Trucks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trucks.each do |truck| %>
      <tr>
        <td><%= truck.applicant %></td>
        <td><%= truck.latitude %></td>
        <td><%= truck.longitude %></td>
        <td><%= link_to 'Show', truck %></td>
      </tr>
    <% end %>
  </tbody>
</table>